[run]
source = django_migrate_fresh
omit = 
    */tests/*
    */test_*.py
    */migrations/*
    */venv/*
    */env/*
    */virtualenv/*
    manage.py
    setup.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
